{
  "name": "webpack3",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.js",
    "lazy": "webpack --config webpack.plugin.lazy.js",
    "cmp": "webpack --config webpack.plugin.cmp.js",
    "lazycmp": "webpack --config webpack.plugin.lazy.cmp.js",
    "test": "webpack --config webpack.config.test.js",
    "ts": "webpack --config webpack.config.ts.js",
    "style": "webpack --config webpack.config.style.js",
    "extract": "webpack --config webpack.plugin.extract.js",
    "postcss": "webpack --config webpack.config.postcss.js",
    "tree": "webpack --config webpack.config.tree.js",
    "filedeal": "webpack --config webpack.config.filedeal.js",
    "html": "webpack --config webpack.config.html.js",
    "htmlinlinechunk": "webpack --config webpack.plugin.htmlinlinechunk.js",
    "watchmode": "webpack --watch --progress --color --display-reasons --config webpack.dev.watchmode.js",
    "devserver": "webpack-dev-server --open --config webpack.dev.devserver.js",
    "dev": "webpack-dev-server --env development --open --config build/webpack.base.conf.js",
    "build": "webpack-dev-server --env production --open --config build/webpack.base.conf.js",
    "start": "node build/server.js",
    "lazy_stats": "webpack --config webpack.plugin.lazy.js --profile --json > src/result-analysis/stats_lazy.json",
    "stats": "webpack --config webpack.result.analysis.js --profile --json > src/result-analysis/stats.json",
    "result": "webpack --config webpack.result.analysis.js",
    "myloader": "webpack --config webpack.myloader.conf.js"
  },
  "sideEffects": false,
  "devDependencies": {
    "autoprefixer": "8.6.4",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.11",
    "cssnano": "4.0.0",
    "eslint": "5.2.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-config-standard": "11.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.18.0",
    "img-loader": "3.0.0",
    "inline-manifest-webpack-plugin": "4.0.0",
    "mini-css-extract-plugin": "0.4.1",
    "opn": "5.3.0",
    "postcss": "6.0.23",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.1.5",
    "postcss-sprites": "4.2.1",
    "prettier": "1.15.3",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.1",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-hot-middleware": "2.22.3",
    "webpack-merge": "4.1.3"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "create-react-app": "1.5.2",
    "jquery": "3.3.1",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "lodash": "4.17.10",
    "node-sass": "4.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "sass-loader": "7.0.3"
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions"
  ]
}
